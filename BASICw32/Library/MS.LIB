! ASC
EXTERNAL FUNCTION ASC(s$)
LET ASC=ORD(s$(1:1))
END FUNCTION
!CINT
EXTERNAL FUNCTION CINT(a)
LET CINT=ROUND(a)
END FUNCTION
! FIX
EXTERNAL FUNCTION FIX(a)
LET FIX=IP(a)
END FUNCTION
! HEX$
EXTERNAL FUNCTION HEX$(n)
LET HEX$=BSTR$(n,16)
END FUNCTION
! INSTR
EXTERNAL FUNCTION INSTR(n,a$,b$)
LET INSTR=POS(a$,b$,n)
END FUNCTION
!  SPACE$
EXTERNAL FUNCTION SPACE$(n)
LET SPACE$=REPEAT$(" ",n)
END FUNCTION
!  STRING$
EXTERNAL FUNCTION STRING$(n,a$)
LET STRING$=REPEAT$(a$(1:1),n)
END FUNCTION
