10 REM 大学入試センター追試験 数学A 2002年
100 INPUT "A=";A
110 C=10
120 IF A/C < 1 THEN GOTO 150
130 C=C*10
140 GOTO 120
150 E=A*A - A
160 IF INT(E/C) = E/C THEN GOTO 190
170 PRINT "F"
180 GOTO 260
190 B = 0
200 FOR D=1 TO 9
210    G= D*C + A
220    E= (G*G -G)/C
230   IF INT(E/10) = E/10 THEN B=G
240 NEXT D
250 PRINT B
260 END

