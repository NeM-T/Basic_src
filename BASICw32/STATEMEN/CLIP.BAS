100 SET VIEWPORT 0.5, 1, 0.5, 1
110 SET WINDOW -4,4,-4,4
120 DRAW GRID
130     SET CLIP "OFF"
140 FOR x=-4 TO 4 STEP 0.01
150    PLOT LINES:x,x^3-3*x;
160 NEXT x
170     SET CLIP "ON"
180 SET VIEWPORT 0,0.5 , 0,0.5
190 SET WINDOW -4,4,-4,4
200 DRAW GRID
210 FOR x=-4 TO 4 STEP 0.01 
220    PLOT LINES:x,2^x;
230 NEXT x
240 END
