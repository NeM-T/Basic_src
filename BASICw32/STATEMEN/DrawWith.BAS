140 DATA 1, 0, 0, 0.1
150 DATA 0, 1, 0,-0.1
160 DATA 0, 0, 1, 0
170 DATA 2, 2, 0, 1 
180 SET WINDOW -100, 100, -100, 100  
190 DRAW axes(10,10)
